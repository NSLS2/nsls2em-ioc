#
# 4 Channel DAC output control
# ao -> calout -> longout --> pscdrv tcp/ip -> ARM tcp server -> FPGA DAC spi control
#

# DAC0 Setpoint +/-10V
record(ao, "$(PriSys){$(PSC)}DAC-Chan1-Sp")
{  
  field(HOPR, "9.999")
  field(DRVH, "9.999")
  field(LOPR, "-9.999")
  field(DRVL, "-9.999")  
  field(VAL , "0") 
  field(FLNK, "$(PriSys){$(PSC)}DAC-Chan1-calcout")
  info(autosaveFields, "VAL")
}
#2nd complement
record(calcout, "$(PriSys){$(PSC)}DAC-Chan1-calcout") {
  field(INPA, "$(PriSys){$(PSC)}DAC-Chan1-Sp")  
  field(CALC, "((1-1)<<16)+((A>=0)?(A*3276.8):(65535+(A*3276.8)))")  
  field(OUT , "$(PriSys){$(PSC)}R210-Sp")  
  field(FLNK, "$(PriSys){$(PSC)}R210-Sp")
}

### PID interface
# Direct integer DAC value (range: -32768 ~ 32767)
record(ao, "$(PriSys){$(PSC)}DAC-Chan1-Cp") {
  field(HOPR, "32767")
  field(DRVH, "32767")
  field(LOPR, "-32768")
  field(DRVL, "-32768")
  field(VAL , "0") 
  field(FLNK, "$(PriSys){$(PSC)}DAC-Chan1-PIDout")
  info(autosaveFields, "VAL")
}
# 2's complement encode for DAC
record(calcout, "$(PriSys){$(PSC)}DAC-Chan1-PIDout") {
  field(INPA, "$(PriSys){$(PSC)}DAC-Chan1-Cp")
  field(CALC, "((1-1)<<16) + (((A>32767)?32767:(A<-32768)?(65536-32768):(A>=0)?A:(65536+A)))")
  field(OUT , "$(PriSys){$(PSC)}R210-Sp")
  field(FLNK, "$(PriSys){$(PSC)}R210-Sp")
  field(PREC, "0")
}
record(longout, "$(PriSys){$(PSC)}R210-Sp") {
  field(DESC,  "R210-DAC1-Sp")
  field(SCAN,  "Passive")
  field(DTYP,  "PSC Single I32")
#  field(TPRO, "1")
  field(OUT , "@CmdPort_$(PSC) 80 840")
  field(FLNK, "$(PriSys){$(PSC)}GoRegSingleWrite")  
}
record(longin,  "$(PriSys){$(PSC)}DAC:X-I") {
    field(DESC, "Reg210, DAC X output")
    field(DTYP, "PSC Reg")
    field(INP , "@RxPort_$(PSC) 82 840")
    field(SCAN, "I/O Intr")
    field(EGU,  "Cnt")
}





